package trycatchfinally;

import java.util.InputMismatchException;
import java.util.Scanner;

public class bora {
	int[] bora = new int[10];
	Scanner sc= new Scanner(System.in);try
	{
		bora[1] = sc.nextInt();
	}catch(
	InputMismatchException e)
	{
		System.out.println("escreve certo porra");
	}sc.close();
}}
